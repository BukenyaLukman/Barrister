<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Attorney.MyApplicationsActivity">

    <include
        layout="@layout/app_bar_layout_main"
        android:id="@+id/applications_app_bar"></include>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/applications_recyclerView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/applications_app_bar">

    </androidx.recyclerview.widget.RecyclerView>

</RelativeLayout>